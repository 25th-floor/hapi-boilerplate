{
  "name": "ttrack-server",
  "version": "0.4.0",
  "description": "Another TimeTracking Tool",
  "main": "src/server/app.js",
  "directories": {
    "bin": "src"
  },
  "scripts": {
    "start": "node --use-strict ./src/server/index.js",
    "lint": "eslint ./src",
    "fix-lint": "eslint --fix ./src",
    "flow": "flow check",
    "test": "jest --no-cache src/server/**/*"
  },
  "author": "25th floor",
  "license": "ISC",
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/jasmine",
      "<rootDir>/node_modules/jasmine-check",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/moment",
      "<rootDir>/node_modules/testcheck"
    ],
    "roots": [
      "./src"
    ]
  },
  "devDependencies": {
    "babel-core": "~6.24",
    "babel-eslint": "~7.2",
    "babel-jest": "~20.0",
    "babel-loader": "~7.0",
    "babel-plugin-transform-class-properties": "~6.24",
    "babel-plugin-transform-function-bind": "~6.22",
    "babel-plugin-transform-object-rest-spread": "~6.23",
    "babel-preset-es2015": "~6.24",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "chakram": "^1.5.0",
    "eslint": "~3.19",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "~1.7",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "flow-bin": "^0.46.0",
    "istanbul": "^0.4.5",
    "jasmine": "~2.6",
    "jasmine-check": "git://github.com/dready/jasmine-check.git#master",
    "jasmine-expect": "~3.7",
    "jasmine-expect-moment": "~0.1",
    "jasmine-reporters": "~2.2",
    "jest-cli": "~20.0",
    "ramda": "^0.24.1"
  },
  "dependencies": {
    "babel-polyfill": "~6.23",
    "body-parser": "^1.15.2",
    "consolidate": "^0.14.5",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "express": "4.15.3",
    "lodash": "^4.17.4",
    "moment": "~2.18.1",
    "moment-duration-format": "^1.3.0",
    "mustache": "^2.3.0",
    "pg": "^6.3.1",
    "pg-types": "^1.11.0",
    "q": "^1.4.1",
    "raven": "^2.0.1",
    "rest": "^2.0.0",
    "sql": "^0.75.0"
  }
}
