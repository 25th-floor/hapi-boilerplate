{
  "name": "ttrack",
  "version": "0.3.1",
  "description": "Another TimeTracking Tool",
  "main": "src/server/app.js",
  "directories": {
    "bin": "src"
  },
  "scripts": {
    "start": "node --use-strict ./src/server/index.js",
    "build": "webpack --config webpack.production.config.js",
    "dev": "webpack-dev-server",
    "lint": "eslint src || true",
    "fix": "eslint --fix src || true",
    "flow": "flow check",
    "test": "NODE_ENV=test node --harmony-proxies `npm bin`/jest --no-cache"
  },
  "author": "25th floor",
  "license": "ISC",
  "engines": {
    "node": ">=v5.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-function-bind",
      "transform-object-rest-spread"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/jasmine",
      "<rootDir>/node_modules/jasmine-check",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/moment",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/testcheck"
    ],
    "testPathDirs": [
      "./src"
    ]
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "~6.21",
    "babel-eslint": "~7.1",
    "babel-jest": "~18.0",
    "babel-loader": "~6.2",
    "babel-plugin-react-intl": "~2.3",
    "babel-plugin-transform-class-properties": "~6.19",
    "babel-plugin-transform-function-bind": "~6.8",
    "babel-plugin-transform-object-rest-spread": "~6.20",
    "babel-preset-es2015": "~6.18",
    "babel-preset-react": "~6.16",
    "babel-preset-react-hmre": "~1.1",
    "copy-webpack-plugin": "~4.0",
    "css-loader": "^0.26.1",
    "eslint": "~3.13",
    "eslint-config-25th": "~6.0",
    "eslint-loader": "~1.6",
    "eslint-plugin-react": "~6.9",
    "extract-text-webpack-plugin": "~1.0",
    "flow-bin": "^0.33.0",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.5",
    "jasmine": "~2.5",
    "jasmine-check": "git://github.com/dready/jasmine-check.git#master",
    "jasmine-expect": "~3.0",
    "jasmine-expect-moment": "~0.1",
    "jasmine-expect-react": "~0.1",
    "jasmine-reporters": "~2.2",
    "jest-cli": "~18.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "path": "^0.12.7",
    "react-addons-pure-render-mixin": "~15.4",
    "react-addons-test-utils": "~15.4",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "~1.14",
    "webpack-dev-server": "~1.16"
  },
  "dependencies": {
    "babel-polyfill": "~6.20",
    "body-parser": "^1.15.2",
    "bootflat": "^2.0.4",
    "bootstrap": "~3.3.7",
    "class-set": "0.0.4",
    "consolidate": "^0.14.5",
    "csv": "^1.1.0",
    "csv-parse": "1.1.8",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "express": "4.14.0",
    "font-awesome": "FortAwesome/Font-Awesome#v4.3.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "~2.17.1",
    "moment-duration-format": "^1.3.0",
    "mustache": "^2.3.0",
    "myro": "^0.6.3-alpha.0",
    "nomnom": "^1.8.1",
    "pg": "^6.1.2",
    "pg-types": "^1.11.0",
    "q": "^1.4.1",
    "raven": "^1.1.1",
    "react": "~15.4",
    "react-dom": "~15.4",
    "rest": "^2.0.0",
    "serve-static": "^1.11.1",
    "sql": "^0.73.0"
  }
}
